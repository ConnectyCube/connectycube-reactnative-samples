{
  "name": "RNChat",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "restart": "yarn start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "xcode": "open ios/RNVideoChatConf.xcworkspace",
    "clean:ios": "rm -f ios/Podfile.lock && rm -rf ios/Pods && rm -rf ios/build",
    "clean:android": "rm -rf android/app/build && rm -rf android/.gradle",
    "clean:gradle": "cd android && ./gradlew --stop && ./gradlew clean && ./gradlew cleanBuild && cd ..",
    "clean:yarn": "rm -rf node_modules/ && rm -f yarn.lock",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-navigation/elements": "^1.3.31",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.8",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-connectycube": "^4.0.0",
    "react-native-device-info": "^13.0.0",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-image-crop-picker": "^0.41.3",
    "react-native-keyboard-controller": "1.14.0",
    "react-native-notifications": "^5.1.0",
    "react-native-reanimated": "^3.15.4",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
