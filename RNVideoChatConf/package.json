{
  "name": "RNVideoChatConf",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "restart": "yarn start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
